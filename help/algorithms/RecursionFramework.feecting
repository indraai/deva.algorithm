func: decisionFunc(key) {
	// set the y item
	const: z = TheData[key]
	context[decision]:{{key}}:{{z.id}}

	try:	{
		action[try]:decision:{{key}}:{{z.id}}
		state[pending]:decision:{{key}}:{{z.id}}
		intent[unknown]:decision:{{key}}:{{z.id}}

		state[z]:decision:TheData:{{key}}:{{z.id}}
		const: #TheData = {{z}} is Resolving True or {{z}} is Iterating to Discover False.
		
		state[data]:decision:TheObservation:{{key}}:{{z.id}}
		const: #TheObservation = #TheData is Creating Cooperation to resolve True or #TheData continue Conflict to hide Falsehoods.
		
	}
	catch: err {
		talk: #error err data
		action[catch]:decision:error:{{key}}:{{data.id}}
		state[invalid]:decision:error:{{key}}:{{data.id}}
		intent[bad]:decision:error:{{key}}:{{data.id}}
		return: this.err(err, packet, reject)
		
	}
	finally {
		action[return]:decision:{{key}}:{{data.id}}
		state[valid]:decision:{{key}}:{{data.id}}
		intent[good]:decision:{{key}}:{{data.id}}
		return: #TheObservation
	}
}

func: OwnerCheck(key) {
	const: owner = TheData[key]
	context[owner]:{{owner.key}}
	action[check]:owner:{{key}}:{{identity.id}} // set the return action
	state[pending]:owner:{{key}}:{{identity.id}} // set the return state valid
	intent[unknown]:owner:{{key}}:{{identity.id}} // set the return intent good

	try: {
		// ask Owner Deva a question to check the identity on for owner properties
		// where y is the data object passed to the question.
		state[check]:owner:{{key}}:{{owner.id}}
		// check the potential owner for owner values and set the OwnerCheck 
		// variable to refer to the returned values.
		talk[OwnerCheck]: #owner check {{owner}}		
	}
	catch: err {
		talk[OwnerCheckError]: #error err {{y}}
		action[catch]:owner:error:{{key}}:{{y.id}}
		state[invalid]:owner:error:{{key}}:{{y.id}}
		intent[bad]:owner:error:{{key}}:{{y.id}}
		return: this.err(err, packet, reject)		
	}
	finally: {
		action[return]:owner:{{key}}:{{identity.id}} // set the return action
		state[valid]:owner:{{key}}:{{identity.id}} // set the return state valid
		intent[good]:owner:{{key}}:{{identity.id}} // set the return intent good
		return: owner;		
	}

}

func: identityCheck(key) {
	const: identity = TheData[key]
	context[identity]:{{identity.key}}

	try: {
		// ask identity a question to validate the identity on y 
		// where y is the data object passed to the question.
		state[check]:identity:{{key}}:{{identity.id}}
		talk[IdentityCheck]: #identity check {{identity}}		
	}
	catch: err {
		talk[IdentityCheckError]: #error err {{identity}}
		action[return]:identity:{{key}}:{{identity.id}}
		state[invalid]:identity:{{key}}:{{identity.id}}
		intent[bad]:identity:{{key}}:{{identity.id}}		
		return: this.err(err, identity, reject)		
	}
	finally {
		action[return]:identity:{{key}}:{{identity.id}}
		state[valid]:identity:{{key}}:{{identity.id}}
		intent[good]:identity:{{key}}:{{identity.id}}
		return: OwnerCheck(identity.key);		
	}
}

func: recursionFunc(key) {
	// set the y item
	const: y = TheData[key]
	context[recursion]:{{key}}:{{y.id}} // set the context to recursion
	state[pending]:recursion:{{key}}:{{data.id}} // set the state pending
	intent[unknown]:recursion:{{key}}:{{data.id}} // set the intent unknown

	
	// then the return the Invalid Identity fun if the check fails.
	state[check]:recursion:identity:{{key}}:{{y.id}}
	const: identityValid = identityCheck(y.key)
	if: !identityValid.check return identityValid
	
	// check to make sure the current item has parents array with length.
	state[check]:recursion:hasParents:{{key}}:{{y.id}}
	const: hasParents = y.parents && y.parents.length
	
	// return function if there are no parents.
	if: !hasParents {
		action[return]:recursion:{{key}}:{{y.id}}
		state[valid]:recursion:{{key}}:{{y.id}}
		intent[good]:recursion:{{key}}:{{y.id}}
		return: decisionFunc(y.key)	
	}
	// run recursion over the y parents array.
	try: {
		action[try]:recursion:parents:{{key}}:{{y.id}}
		state[parent]:recursion:parents:{{key}}:{{y.id}}
		intent[unknown]:recursion:parents:{{key}}:{{y.id}}
		state[loop]:recursion:parents:{{key}}:{{y.id}}
		for: parent in y.parents {
			action[return]:recursion:parents:parent:{{parent.key}}:{{parent.id}}
			return: recursionFunc(parent.key)
		}
	}	
	catch: err {
		talk[RecursionError]: #error err {{y}}
		action[catch]:recursion:error:{{key}}:{{y.id}}
		state[invalid]:recursion:error:{{key}}:{{y.id}}
		intent[bad]:recursion:error:{{key}}:{{y.id}}
		return: this.err(err, packet, reject)
	}
	finally: {
		action[finally]:recursion:final:{{key}}:{{y.id}}
		state[valid]:recursion:final:{{key}}:{{y.id}}
		intent[good]:recursion:final:{{key}}:{{y.id}}
		return: decisionFunc(y.key)
	}
}

// set the a action marker
const: a = 'item3'

// loop over The Object with for of loop.
for: x of TheData {
	if: (a !== x.key) continue
	// set the value of x key lookup into a local data object.	
	try: {
		action[try]:x.id
		state[pending]:x.id
		intent[unknown]:x.id
		recursionFunc(x.key)	
	}	
	catch: err {
		action[catch]:x.id
		state[invalid]:x.id
		intent[bad]:x.id
		talk: #error err x
		return: this.err(err, packet, reject)
	}
	finally: {
		action[finally]:x.id
		state[valid]:x.id
		intent[good]:x.id
		return: x
	}
}
::end:example

